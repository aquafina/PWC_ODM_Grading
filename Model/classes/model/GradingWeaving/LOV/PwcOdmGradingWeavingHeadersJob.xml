  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.GradingWeaving.LOV.PwcOdmGradingWeavingHeadersJob_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_gd_no"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="p_sales_order"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[select  sum(mmt.TRANSACTION_QUANTITY) job_quantity    
        ,sum(wdj.QUANTITY_COMPLETED) stitching_quantity
        ,we.WIP_ENTITY_NAME
        ,msi.concatenated_segments
        ,msi.DESCRIPTION
from   wip_discrete_jobs wdj
       ,wip_entities we
       ,mtl_material_transactions mmt
       ,mtl_transaction_types  mtt
       ,mtl_system_items_KFV msi
where  we.WIP_ENTITY_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_SOURCE_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_TYPE_ID = mtt.TRANSACTION_TYPE_ID
and    upper(mtt.TRANSACTION_TYPE_NAME) = 'WIP COMPLETION'
and wdj.status_type = 3
and mmt.INVENTORY_ITEM_ID = msi.INVENTORY_ITEM_ID
and mmt.ORGANIZATION_ID = msi.ORGANIZATION_ID
and wdj.ATTRIBUTE11 = nvl(:p_gd_no,wdj.ATTRIBUTE11)
and wdj.ATTRIBUTE12 = nvl(:p_sales_order,wdj.ATTRIBUTE12)                   
group by we.WIP_ENTITY_NAME,msi.concatenated_segments,msi.DESCRIPTION
ORDER BY 3]]>
  </SQLQuery>
