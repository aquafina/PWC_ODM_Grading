<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PwcOdmGradingWeavingHeadersJob"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  OrderBy="3"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.GradingWeaving.LOV.PwcOdmGradingWeavingHeadersJob_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select  sum(mmt.TRANSACTION_QUANTITY) job_quantity    
        ,sum(wdj.QUANTITY_COMPLETED) stitching_quantity
        ,we.WIP_ENTITY_NAME 
from   wip_discrete_jobs wdj
       ,wip_entities we
       ,mtl_material_transactions mmt
       ,mtl_transaction_types  mtt
where  we.WIP_ENTITY_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_SOURCE_ID = wdj.WIP_ENTITY_ID
and    mmt.TRANSACTION_TYPE_ID = mtt.TRANSACTION_TYPE_ID
and    upper(mtt.TRANSACTION_TYPE_NAME) = 'WIP COMPLETION'                  
group by we.WIP_ENTITY_NAME]]>
  </SQLQuery>
  <ViewAttribute
    Name="JobQuantity"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="JOB_QUANTITY"
    Expression="JOB_QUANTITY"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="StitchingQuantity"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="STITCHING_QUANTITY"
    Expression="STITCHING_QUANTITY"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="WipEntityName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WIP_ENTITY_NAME"
    Expression="WIP_ENTITY_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
